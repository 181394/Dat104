<?xml version="1.0" encoding="iso-8859-1"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <context-param>
        <param-name>pass</param-name>
        <param-value>admin</param-value>
    </context-param>
    <display-name>Fest</display-name>
    <welcome-file-list>
        <welcome-file>/</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>KassererServlet</servlet-name>
        <servlet-class>no.hib.dat104.controller.KassererServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>KassererServlet</servlet-name>
        <url-pattern>/Kasserer</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>no.hib.dat104.controller.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>OversiktServlet</servlet-name>
        <servlet-class>no.hib.dat104.controller.OversiktServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OversiktServlet</servlet-name>
        <url-pattern>/Oversikt</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>PaameldingServlet</servlet-name>
        <servlet-class>no.hib.dat104.controller.PaameldingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PaameldingServlet</servlet-name>
        <url-pattern>/Paamelding</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>no.hib.dat104.controller.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>BetalingsServlet</servlet-name>
        <servlet-class>no.hib.dat104.controller.BetalingsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BetalingsServlet</servlet-name>
        <url-pattern>/Betaling</url-pattern>
    </servlet-mapping>
</web-app>